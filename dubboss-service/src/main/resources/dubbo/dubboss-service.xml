<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

     <dubbo:application name="dubboss-service" />
     <dubbo:registry protocol="zookeeper" address="10.1.11.147:2888" />
     <dubbo:protocol name="dubbo" port="20880" />
     <dubbo:service interface="cn.injava.dubboss.api.DemoService" ref="demoService" />
     <dubbo:service interface="cn.injava.dubboss.api.StubTestService" ref="stubTestService" stub="true"/>
     <bean id="demoService" class="cn.injava.dubboss.service.impl.DemoServiceImpl" />
     <bean id="stubTestService" class="cn.injava.dubboss.service.impl.StubTestServiceImpl"/>
</beans>